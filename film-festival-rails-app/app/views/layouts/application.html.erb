<!DOCTYPE html>
<html>
<head>
  <title>FilmFestivalRailsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= link_to "LES Film Festival", root_path %>
<%= link_to "Categories", categories_path %>
<%= link_to "Judges", judges_path %>
<% if logged_in? %>
  <%= link_to "Logout", logout_path %>
  <%= link_to "Profile", user_path %>
<% else %>
  <%= link_to "Register", new_user_path %>
  <%= link_to "Login", login_path %>
<% end %>
<%= yield %>
<%= link_to "Judges Registration", judges_register_path %>
<%= link_to "Judges Login", judges_login_path %>
</body>
</html>
